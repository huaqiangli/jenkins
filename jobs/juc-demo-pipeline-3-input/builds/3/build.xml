        					rm -rf testreport/*
        					echo &quot;success&quot; &gt; testreport/test-report.html
        				&apos;&apos;&apos;
        				stash includes: &apos;**/testreport/*.html&apos;, name: &apos;report&apos;
        			}
        		}
